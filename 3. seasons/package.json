    "rules": {
      "react/jsx-one-expression-per-line": "off",
      "react/state-in-constructor": "off"
    },
    "parser": "@babel/eslint-parser"
  },
  "babel": {
    "presets": [
      "airbnb"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "resolutions": {
    "react-scripts/eslint-webpack-plugin": "2.4.1"
  }
